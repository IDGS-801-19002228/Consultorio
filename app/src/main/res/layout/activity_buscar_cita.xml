<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:theme="@style/Theme.MyApplication"
    tools:context="com.example.consultorio.activities.buscarCita">


    <fragment
        android:id="@+id/fragment"
        android:name="com.example.consultorio.fragments.pecesFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/btnHome"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="290dp"
            android:layout_marginTop="400dp"
            android:layout_marginEnd="209dp"
            android:layout_marginBottom="0dp"
            android:backgroundTint="#4CD6B7"
            android:onClick="volverHome"
            android:src="@drawable/homebuttom"
            app:borderWidth="0dp"
            app:elevation="4dp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>